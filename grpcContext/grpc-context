package grpcContext

import (
	"log"

	"google.golang.org/grpc"
	"github.com/j-ew-s/ms-curso-user-api"
)



func Setup() {
	var cc *grpc.ClientConn

	cc, err:= grpc.Dial(":5001", grpc.WithInsecure())

	if err != nil{
		log.Fatal("gRPC DIAL falhou: %v", err)
	}

	defer cc.Close
	c := user.NewChatServiceClient(cc)

	return c

}